{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["name", "uri"],
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1
      },
      "uri": {
        "type": "string",
        "format": "uri",
        "pattern": "^https://.*[?&](q|query|p)=$"
      }
    },
    "additionalProperties": false
  }
}
